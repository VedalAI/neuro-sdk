<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>VedalAI.NeuroSdk.Unity</id>
        <version>$version$</version>
        <title>Neuro-sama Game SDK for Unity</title>
        <authors>Vedal AI</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <license type="expression">MIT</license>
        <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
        <icon>images\icon.png</icon>
        <readme>_content/README.md</readme>
        <releaseNotes>https://github.com/VedalAI/neuro-game-sdk/blob/main/CHANGELOG.md</releaseNotes>
        <description>Neuro-sama Game SDK for Unity, designed to be used for modding Mono games with BepInEx.</description>
        <copyright>Copyright (C) Vedal AI 2024-2025</copyright>
        <repository type="git" url="https://github.com/VedalAI/neuro-game-sdk" branch="$branch$" commit="$commit$" />
    </metadata>
    <files>
        <file target="_content\" src="..\README.md" />
        <file target="images\" src="..\..\Assets\icon.png" />

        <file target="_res\" src="Resources\*.dll" />
        <file target="build\" src="VedalAI.NeuroSdk.Unity.props" />
        <file target="build\" src="VedalAI.NeuroSdk.Unity.targets" />
        <file target="contentfiles\cs\any\NeuroSdk" src="..\Assets\**\*.cs" exclude="..\Assets\Examples\**\*.cs" />
        <file target="contentfiles\cs\any\NeuroSdk\Modding" src="Source\**\*.cs" />
    </files>
</package>
